{
    "name": "vue3.x-vite-template",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "prepare": "husky install"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue,jsx,less,scss,json}": [
            "prettier --write"
        ],
        "*.ts?(x)": [
            "prettier --parser=typescript --write"
        ]
    },
    "dependencies": {
        "ant-design-vue": "^2.2.8",
        "element-plus": "^1.3.0-beta.5",
        "normalize.css": "^8.0.1",
        "vue": "^3.2.27",
        "vue-router": "4",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@types/node": "^17.0.8",
        "@vitejs/plugin-vue": "^2.0.1",
        "@vue/compiler-sfc": "^3.2.27",
        "autoprefixer": "^10.4.2",
        "husky": "^7.0.4",
        "less": "^4.1.2",
        "lint-staged": "^12.1.7",
        "postcss": "^8.4.5",
        "prettier": "2.5.1",
        "sass": "^1.48.0",
        "typescript": "^4.5.4",
        "unplugin-vue-components": "^0.17.13",
        "vite": "^2.7.12",
        "vue-tsc": "^0.30.5"
    }
}
